syntax = "proto3";

package {{ .ServiceName }};

{{- range .Includes }}
import "{{ . }}";
{{- end }}

{{- range .Structs }}

message {{ .Name }} {
{{- range .Fields }}
	{{ .Type.Name}} {{ .Name }} = {{ .ID }};
{{- end }}
}

{{- end }}

service {{ .ServiceName }} {
	rpc Ping(SendMailPingResp) returns (ReceiveMailPingResp);
}
