syntax = "proto3";

package {{ .ServiceName }};

{{- range .Includes }}
import "{{ . }}";
{{- end }}

{{- range .Thrift.Enums }}

enum {{ .Name }} {
{{- range ListEnumValue .Values }}
	{{ .Name }} = {{ .Value }};
{{- end }}
}
{{- end }}

{{- range .GetStructs }}

message {{ .Name }} {
{{- range .GetFields }}
	{{ .GetType}} {{ .Name }} = {{ .ID }};
{{- end }}
}

{{- end }}

service {{ .ServiceName }} {
	rpc Ping(SendMailPingResp) returns (ReceiveMailPingResp);
}
